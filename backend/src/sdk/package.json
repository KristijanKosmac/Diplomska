{
    "name": "pet-backend-sdk",
    "version": "0.11.0",
    "description": "PET Backend SDK",
    "main": "axios/index.js",
    "typings": "axios/index",
    "files": [
        "axios/"
    ],
    "scripts": {
        "prepublishOnly": "npm run build",
        "prebuild": "rm -rf ./axios && npm run generate-sdk",
        "build": "tsc -p  ./",
        "generate-sdk": "npm run generate-axios-api && sed -i -e 's/accessToken?: string | ((name: string/accessToken?: string | ((name?: string/g' src/axios/configuration.ts",
        "postgenerate-sdk": "rm -r ./src/**/.openapi-generator ./src/**/.gitignore ./src/**/*.sh ./src/**/.openapi-generator-ignore ./src/**/*-e",
        "pregenerate-sdk": "rm -r ./src ; npm i",
        "generate-axios-api": "openapi-generator generate -i ../../../../swagger.json -g typescript-axios -o ./src/axios"
    },
    "author": "Microtica",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.19.0"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "1.0.8-4.2.2",
        "typescript": "^3.5.3"
    }
}
