{
  "name": "pet",
  "version": "0.0.1",
  "description": "PET component",
  "main": "dst/index.js",
  "scripts": {
    "build": "tsc -p ./src/tsconfig.json",
    "pretest": "npm run build",
    "start": " databaseName=d databaseHost=d databasePort=3306 databaseUsername=d s3BucketName=kristijan-xray  fromEmail=antonio.todorov@microtica.com node dst/server.js",
    "test": "npm run mocha",
    "posttest": "npm run lint",
    "mocha": "./node_modules/.bin/nyc --extension=.ts --reporter=lcov --reporter=text-summary node_modules/.bin/mocha --recursive dst/test --bail --sort --timeout 10000",
    "prepackage": "rm -rf package.zip && npm i && npm run build && npm prune --production",
    "package": "zip -r package.zip dst node_modules package.json ",
    "postpackage": "echo File size: $(ls -lah package.zip | awk '{print $5}')",
    "lint": "tslint -p src/tsconfig.json -c node_modules/@microtica/tslint-config/tslint.json --fix",
    "coverage": "open coverage/lcov-report/index.html",
    "generate-sdk": "cd src/sdk && npm run generate-sdk && cd ../..",
    "publish-sdk": "cd src/sdk && npm version minor && npm publish && cd ../../.."
  },
  "nyc": {
    "check-coverage": true,
    "lines": 20,
    "statements": 20,
    "functions": 20,
    "watermarks": {
      "branches": [
        10,
        50
      ],
      "lines": [
        90,
        100
      ],
      "functions": [
        90,
        100
      ],
      "statements": [
        90,
        100
      ]
    }
  },
  "author": "Microtica",
  "license": "ISC",
  "dependencies": {
    "@microtica/component": "^2.2.0",
    "@microtica/logger": "0.0.1",
    "@microtica/utils": "^1.3.2",
    "nodemailer": "^6.6.3",
    "aws-sdk": "^2.466.0",
    "aws-serverless-express": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "http-status-codes": "^1.4.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@microtica/tslint-config": "^0.1.0",
    "@types/aws-lambda": "^8.10.26",
    "@types/aws-serverless-express": "^3.3.2",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/mock-require": "^2.0.0",
    "@types/multer": "^1.4.6",
    "@types/multer-s3": "^2.7.9",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^3.4.6",
    "@types/woocommerce__woocommerce-rest-api": "^1.0.0",
    "aws-sdk": "^2.423.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1",
    "tslint": "^5.7.0",
    "typescript": "^3.2.2"
  }
}
